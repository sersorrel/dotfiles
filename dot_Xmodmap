keycode  10 = 0 exclam 0 exclam onesuperior exclamdown
keycode  11 = 1 quotedbl 1 at twosuperior oneeighth
keycode  12 = 2 sterling 2 numbersign threesuperior sterling
keycode  13 = 3 dollar 3 dollar EuroSign onequarter
keycode  14 = 4 percent 4 percent onehalf threeeighths
keycode  15 = 5 asciicircum 5 asciicircum threequarters fiveeighths
keycode  16 = 6 ampersand 6 ampersand braceleft seveneighths
keycode  17 = 7 asterisk 7 asterisk bracketleft trademark
keycode  18 = 8 parenleft 8 parenleft bracketright plusminus
keycode  19 = 9 parenright 9 parenright braceright degree
